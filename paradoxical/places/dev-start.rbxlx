	self:writeEnumInput(virtualAxis2d.up)
	self:writeEnumInput(virtualAxis2d.down)
	self:writeEnumInput(virtualAxis2d.left)
	self:writeEnumInput(virtualAxis2d.right)
end

return Serializer
]]></ProtectedString>
