	if __DEV__ and enableDoubleInvokingEffects then
		mountEffectImpl(bit32.bor(MountLayoutDevEffect, UpdateEffect), HookLayout, create, deps)
	else
		mountEffectImpl(UpdateEffect, HookLayout, create, deps)
	end
