		.. Squash.EnumItem.ser(x.EasingStyle, Enum.EasingStyle)
		.. Squash.EnumItem.ser(x.EasingDirection, Enum.EasingDirection)
		.. Squash.uint.ser(x.RepeatCount, bytes)
